{
    "name": "proyecto404/gigya-bundle",
    "description": "Bundle around the Gigya PHP SDK",
    "type": "symfony-bundle",
    "keywords": ["gigya", "authentication"],
    "homepage": "https://github.com/proyecto404/GigyaBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Bottarini",
            "email": "nicolasbottarini@gmail.com",
            "homepage": "http://www.nicolasbottarini.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "proyecto404/gigya-php-sdk": ">= dev-master"
    },
    "target-dir": "Proyecto404/GigyaBundle",
    "autoload": {
        "psr-4": {
            "Proyecto404\\GigyaBundle": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}